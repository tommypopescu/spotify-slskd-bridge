# Spotify (Client Credentials) — necesar oricum pentru fallback
SPOTIFY_CLIENT_ID=changeme
SPOTIFY_CLIENT_SECRET=changeme

# Spotify OAuth — Authorization Code Flow (user)
# Setează redirect-ul exact în Spotify Developer Dashboard (vezi README)
SPOTIFY_REDIRECT_URI=http://127.0.0.1:8080/callback
SPOTIFY_USER_SCOPE=user-read-private

# Context piață (pentru requests fără user; folosit ca fallback)
SPOTIFY_MARKET=RO

# slskd (API)
SLSKD_BASE=http://192.168.1.7:5030
SLSKD_API_KEY=changeme